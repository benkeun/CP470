<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidassignments">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/android"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/android_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MessageDetails"></activity>
        <activity android:name=".WeatherForecast" />
        <activity
            android:name=".TestToolbar"
            android:label="@string/title_activity_test_toolbar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ChatWindow" />
        <activity android:name=".ListItemsActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>